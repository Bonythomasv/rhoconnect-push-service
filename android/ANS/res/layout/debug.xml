<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical" >

    <TextView
        android:id="@+id/status"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Network Status"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/statusText"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="- unknown -"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/fetch"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Fetching Status"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/fetchStatus"
        android:layout_width="match_parent"
        android:layout_height="36dp"
        android:text="- unknown -"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/tokenCheckLabel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Token Check Status"
        android:textAppearance="?android:attr/textAppearanceMedium" />

	<TextView
        android:id="@+id/tokenCheckStatus"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="- unknown -"
        android:textAppearance="?android:attr/textAppearanceSmall" />
	
    <LinearLayout
        android:id="@+id/linearLayout1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:id="@+id/instanceId"
            android:layout_width="161dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.67"
            android:text="Instance IDs"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:id="@+id/instanceIdRetry"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Retry Count"
            android:textAppearance="?android:attr/textAppearanceSmall" />
    </LinearLayout>

    <TextView
        android:id="@+id/instanceIdText"
        android:layout_width="match_parent"
        android:layout_height="36dp"
        android:text="- unknown -"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:id="@+id/instanceCookie"
            android:layout_width="210dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.50"
            android:text="Instance Cookies"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <TextView
            android:id="@+id/instanceCookieRetry"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Retry Count"
            android:textAppearance="?android:attr/textAppearanceSmall" />
    </LinearLayout>

    <TextView
        android:id="@+id/instanceCookieText"
        android:layout_width="match_parent"
        android:layout_height="36dp"
        android:text="- none -"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:id="@+id/registrations"
            android:layout_width="174dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.67"
            android:text="Registrations"
            android:textAppearance="?android:attr/textAppearanceLarge" />

        <TextView
            android:id="@+id/registrationsRetry"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Retry Count"
            android:textAppearance="?android:attr/textAppearanceSmall" />
    </LinearLayout>

    <TextView
        android:id="@+id/numRegistered"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Number Registered"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/numRegisteredText"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="- unknown -"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/regQueue"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Registration Queue"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/regQueueStatus"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="- unknown -"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/regQueueText"
        android:layout_width="match_parent"
        android:layout_height="52dp"
        android:text="- unknown -"
        android:textAppearance="?android:attr/textAppearanceSmall" />

    <TextView
        android:id="@+id/regs"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Registrations"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/regsText"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:text="- unknown -"
        android:textAppearance="?android:attr/textAppearanceSmall" />

</LinearLayout>
